<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.staff.fragments.bottom_nav.StaffHomeFragment">

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="@dimen/_10sdp"
        android:indeterminateDrawable="@drawable/progress_bar_red"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:baselineAligned="false"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="3"
        android:paddingTop="@dimen/_5sdp"
        android:paddingBottom="@dimen/_5sdp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/llCarCompanyMaster"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_car_primary_red" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:text="Car Company\nMaster"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llCustomerMaster"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_baseline_person_add_24" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Customer\nMaster"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llServiceAdvisorMaster"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_mechanical_and_services" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Service Advisor\nMaster"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:baselineAligned="false"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="3"
        android:paddingTop="@dimen/_5sdp"
        android:paddingBottom="@dimen/_5sdp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <LinearLayout
            android:id="@+id/llNotificationMaster"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_baseline_notifications_active_24" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Notification\nMaster"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llServiceLogMaster"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_baseline_miscellaneous_services_24" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Service Log\nMaster"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llPostServiceFollowUp"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_doc_test" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Post Service\nFollow Ups"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3"
        android:paddingTop="@dimen/_5sdp"
        android:paddingBottom="@dimen/_5sdp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout6"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <LinearLayout
            android:id="@+id/llCarHealthReport"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_health_report" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Car Health\nReports"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llInvoices"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_invoice" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Customer\nInvoices"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llEnquiryAndComplaints"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_enquiry" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Enquiry\nAnd\nComplaints"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3"
        android:paddingTop="@dimen/_5sdp"
        android:paddingBottom="@dimen/_5sdp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout8"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

        <LinearLayout
            android:id="@+id/llBeforeAfter"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_before_after" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Before\nAnd\nAfter"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llServiceLogHistory"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_logs" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Service Logs\nHistory"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llStatistics"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_before_after" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Cars 360\nStatistics"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="3"
        android:paddingTop="@dimen/_5sdp"
        android:paddingBottom="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <LinearLayout
            android:id="@+id/llAllCustomers"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_before_after" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="All\nCustomers"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llAllServiceAdvisors"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_logs" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="All Service\nAdvisors"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llAdBanner"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/red_seperator_bg_vertical"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_margin="@dimen/_2sdp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_before_after" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_1sdp"
                android:gravity="center"
                android:lines="3"
                android:text="Advertisment\nBanner"
                android:textAppearance="@style/TextAppearance.Material3.BodySmall"
                android:textColor="@color/primaryRed" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>